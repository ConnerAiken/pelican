{
  "name": "react-node-webpack-boilerplate",
  "version": "0.0.0-0",
  "description": "React/Nodejs + Webpack",
  "main": "build/bundle.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "webpack --config webpack.common.js --mode production",
    "dev": "npm run start:dev && webpack-dev-server --config webpack.common.js --mode development --open --watch",
    "deploy": "ansible-galaxy install -r requirements.yml && ansible-playbook playbooks/deploy.yml -i playbooks/inventory.yml",
    "test": "echo \"Error: no test specified but lets exit with success so travis builds\" && exit 0",
    "prestart": "npm run build",
    "start:dev": "NODE_ENV=development babel-node src/server/index.js  --presets=env,stage-0",
    "start": "babel-node src/server/index.js  --presets=env,stage-0",
    "start:docker": "docker-compose up",
    "upgradeDeps": "ncu -u",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FittedTech/react-node-webpack.git"
  },
  "keywords": [
    "react",
    "express",
    "node",
    "webpack",
    "webpack 4",
    "mern",
    "boilerplate"
  ],
  "author": "Conner Aiken",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FittedTech/react-node-webpack/issues"
  },
  "homepage": "https://github.com/FittedTech/react-node-webpack#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "node-sass": "^4.9.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.0.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.22.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "file-loader": "^2.0.0",
    "greenlock-express": "^2.4.6",
    "history": "^4.7.2",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "mdbreact": "^4.7.0",
    "react-google-maps": "^9.4.5",
    "react-piwik": "^1.5.0",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^6.4.0",
    "simple-oauth2": "^2.2.1",
    "toastr": "^2.1.4"
  }
}
